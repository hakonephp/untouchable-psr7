{
    "name": "hakone/untouchable-psr7",
    "description": "PSR-7 message implementation with all method calls restricted",
    "license": "Apache-2.0",
    "require": {
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.8",
        "php-http/discovery": "^1.20",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.5",
        "symplify/easy-coding-standard": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "Hakone\\Http\\Message\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
